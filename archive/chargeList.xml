                <colorTriggerBgColor>#000000</colorTriggerBgColor>
                <regexCodeList>
                    <string>[ID]      [Date]     [Enemy]         [Charge]             [Fine]     [Term]    </string>
                    <string>-------------------------------------------------------------------------------</string>
                </regexCodeList>
                <regexCodePropertyList>
                    <integer>3</integer>
                    <integer>3</integer>
                </regexCodePropertyList>
