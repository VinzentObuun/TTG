                <name>city charges start</name>
                <script>enableTrigger(&quot;city charge capture&quot;)

chargeFineTotal = 0
chargeTermTotal = 0</script>
                <triggerType>0</triggerType>
                <conditonLineDelta>1</conditonLineDelta>
                <mStayOpen>0</mStayOpen>
                <mCommand></mCommand>
                <packageName></packageName>
                <mFgColor>#ff0000</mFgColor>
