                <name>city charge capture</name>
                <script>if matches[2] then chargeFineTotal = chargeFineTotal + tonumber(matches[2]) end
if matches[3] then chargeTermTotal = chargeTermTotal + tonumber(matches[3]) end
enableTrigger(&quot;city charge end&quot;)</script>
                <triggerType>0</triggerType>
